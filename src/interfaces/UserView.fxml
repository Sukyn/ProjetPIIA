<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox fx:controller="Controls.UserViewControl" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="labelUsername" alignment="CENTER" prefHeight="16.0" prefWidth="90.0" VBox.vgrow="ALWAYS" />
      <ImageView fitHeight="90.0" fitWidth="90.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="buttonEdit" mnemonicParsing="false" onAction="#editUser" prefHeight="20.0" prefWidth="90.0" text="Modifier" />
      <Button fx:id="buttonDelete" mnemonicParsing="false" onAction="#removeUser" prefHeight="20.0" prefWidth="90.0" text="Supprimer" />
   </children>
</VBox>
